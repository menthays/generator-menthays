{
  "name": "generator-menthays",
  "version": "0.6.2",
  "description": "A simple multi-page webapp boilerplate, with modern frontend tools",
  "homepage": "https://github.com/menthays/generator-menthays",
  "author": {
    "name": "Hao Yang",
    "email": "tillohao@gmail.com",
    "url": "https://menthays.netlify.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "",
    "yeoman-generator"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^4.2.1",
    "nsp": "^3.2.1",
    "prettier": "^1.13.7",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.8.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "yeoman-generator": "^3.0.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint . --fix",
    "precommit": "lint-staged",
    "test": "jest ./__tests__/"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "warn",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "menthays/generator-menthays",
  "license": "MIT"
}
